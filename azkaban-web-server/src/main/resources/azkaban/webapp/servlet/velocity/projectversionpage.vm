<!-- projectversionpage.vm -->
<!DOCTYPE html>
<html lang="en">
<head>
  <meta http-equiv="Pragma" content="no-cache">
  <meta http-equiv="Cache-Control" content="no-cache, must-revalidate">
  <meta http-equiv="Expires" content="0">
  #parse ("azkaban/webapp/servlet/velocity/style.vm")
  #parse ("azkaban/webapp/servlet/velocity/javascript.vm")

  <script type="text/javascript">
    var currentTime = ${currentTime};
    var timezone = "${timezone}";
    var errorMessage = null;
    var successMessage = null;

    var projectId = ${project.id};
    var projectName = "$project.name";
    var langType = "${currentlangType}";
    var downloadBtnText = "${downloadBtn}";
    // load cmdb data switch
    var isCmdbLoaded=false;
  </script>
  <script type="text/javascript" src="/js/azkaban/i18n-config.js?version=1.18.0"></script>
  <script type="text/javascript" src="/js/azkaban/util/date.js?version=1.18.0"></script>
  <script type="text/javascript" src="/js/azkaban/util/ajax.js?version=1.18.0"></script>
  <script type="text/javascript" src="/js/azkaban/view/project-versions.js?version=1.18.0"></script>
  <script type="text/javascript" src="/js/azkaban/view/project-modals.js?version=1.18.0"></script>
  <link href="/css/bootstrap-select.min.css?version=1.18.0" rel="stylesheet">
  <script type="text/javascript" src="/js/bootstrap-select.js?version=1.18.0"></script>
  <script type="text/javascript" src="/js/moment.min.js?version=1.18.0"></script>
  <script type="text/javascript" src="/js/bootstrap-datetimepicker.min.js?version=1.18.0"></script>
  
</head>
<body>

  #set ($current_page="all")
  #parse ("azkaban/webapp/servlet/velocity/nav.vm")

  #if ($errorMsg)
    #parse ("azkaban/webapp/servlet/velocity/errormsg.vm")
  #else

  ## Page header.

    #parse ("azkaban/webapp/servlet/velocity/projectpageheader.vm")

  ## Page content.

  <div class="container-full">

    #parse ("azkaban/webapp/servlet/velocity/alerts.vm")

    <div class="row row-offcanvas row-offcanvas-right">
      <div class="col-xs-12 col-sm-9">

        #set ($project_page = "versions")
        #parse ("azkaban/webapp/servlet/velocity/projectnav.vm")

        <div class="panel panel-default" id="projectVersionView">
          <table class="table table-striped" id="versionTable">
            <thead>
            <tr>
              <th>$versionCol</th>
              <th>$uploadTimeCol</th>
              <th>$operationCol</th>
            </tr>
            </thead>
            <tbody>
            </tbody>
          </table>
        </div>
      </div>
      <div class="col-xs-6 col-sm-3 sidebar-offcanvas">
        #parse ("azkaban/webapp/servlet/velocity/projectsidebar.vm")
      </div>
    </div>

    #parse ("azkaban/webapp/servlet/velocity/projectmodals.vm")

  </div><!-- /container-full -->
  #end
<script>
  $("#start-all-flow-btn").hide()
  $("#start-all-schedule-btn").hide()
</script>
</body>
</html>
