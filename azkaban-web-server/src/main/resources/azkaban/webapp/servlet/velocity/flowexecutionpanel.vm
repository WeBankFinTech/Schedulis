                    <a href="#">$settingsFail</a>
                    <div class="menu-caption">$failedOperate</div>
                  </li>
                  <li viewpanel="concurrent-panel">
                    <a href="#">$concurrentSet</a>
                    <div class="menu-caption">$changeAction</div>
