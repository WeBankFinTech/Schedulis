                        <option value="2">Level 2</option>
                      </select>
                      <span class="help-block">
                        $setChannel
                        <li>
                          <li>$optChannelone</li>
                          <li>$optChanneltwo</li>
                        </li>
                      </span>
                    </div>
                  </div>

