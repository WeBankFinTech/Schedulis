## Modal dialog to be displayed to create a new project.

<div class="modal" id="update-system-user-panel">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;
        </button>
        <h4 class="modal-title">$updateSystemUser</h4>
      </div>
      <div class="modal-body">
        <div class="alert alert-danger" id="update-user-modal-error-msg" style="word-break:break-all;"></div>
        <fieldset class="form-horizontal">
          <div class="form-group">
            <label for="description" class="col-sm-2 control-label">$userId</label>
            <div class="col-sm-8">
              <input id="wtss-user-id" type="text" class="form-control" disabled="disabled">
            </div>
          </div>
          <div class="form-group">
            <label for="description" class="col-sm-2 control-label">$fullName</label>
            <div class="col-sm-8">
              <input id="wtss-full-name" type="text" class="form-control" disabled="disabled">
            </div>
          </div>
          <div class="form-group">
            <label for="description" class="col-sm-2 control-label">$userCategory</label>
            <div class="col-sm-8">
              <input id="wtss-user-category" type="text" class="form-control" disabled="disabled">
            </div>
          </div>
          <div class="form-group">
            <label for="path" class="col-sm-2 control-label">$password</label>
            <div class="col-sm-8">
              <input id="update-password" type="password" class="form-control" maxlength="20">
            </div>
          </div>
          <div class="form-group">
            <label for="description" class="col-sm-2 control-label">$userRole</label>
            <div class="col-sm-8">
              <select id="update-user-role-select" class="form-control">
                <option value="0">$selectRole</option>
                <option value="1">$admin</option>
                <option value="2">$ordinaryUsers</option>
              </select>
            </div>
          </div>
          <div class="form-group">
            <label for="description" class="col-sm-2 control-label">$proxyUser</label>
            <div class="col-sm-8">
              <input id="update-proxy-user" name="proxy-user" type="text" class="form-control" maxlength="250">
            </div>
          </div>
          <div class="form-group">
            <label for="description" class="col-sm-2 control-label">$dep</label>
            <div class="col-sm-8">
##              <select id="update-wtss-department-select" class="form-control" style="width: 100%">
##                <option value="">请选择</option>
##              </select>
              <select id="update-wtss-department-select" class="form-control" style="width: 100%">
                <option value="0">$selectDep</option>
##                #foreach ($deparment in $webankDepartmentList)
##                  <option value=$deparment.dpId>$deparment.dpName</option>
##                #end
              </select>
            </div>
          </div>
        </fieldset>
      </div>
      <div class="modal-footer">
        <input name="action" type="hidden" value="create">
        <input name="redirect" type="hidden" value="$!context/">
        <button type="button" class="btn btn-default" data-dismiss="modal">$cancel</button>
        <button type="button" class="btn btn-primary" id="system-user-update-btn">$update</button>
        <button type="button" class="btn btn-danger" id="system-user-delete-btn">$deleteUser</button>
      </div>
    </div>
  </div>
</div>


